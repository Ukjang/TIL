    "def solution(number):\n",
    "    from itertools import combinations \n",
    "    comb_list = list(combinations(number, 3))\n",
    "    answer = 0\n",
    "    for ls in comb_list:\n",
    "        if sum(ls) == 0:\n",
    "            answer += 1\n",
    "    return answer"
   ]
