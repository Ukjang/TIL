    "def solution(k, score):\n",
    "    # 정렬해서 상위 n개 자른 후\n",
    "    # 그 최소값 리스트에 추가\n",
    "    answer = []\n",
    "    for i in range(len(score)):\n",
    "        temp_score = score[:i+1]\n",
    "        temp_score.sort(reverse=True)\n",
    "        answer.append(min(temp_score[:k]))\n",
    "    return answer"
   ]
  }
