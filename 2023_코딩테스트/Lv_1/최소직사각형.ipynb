    "def solution(sizes):\n",
    "    # 최소값 끼리의 max\n",
    "    # 최대값 끼리의 max\n",
    "    res_min = 0\n",
    "    res_max = 0\n",
    "    for size in sizes :\n",
    "        temp_min = min(size)\n",
    "        temp_max = max(size)\n",
    "        res_min = max(res_min, temp_min)\n",
    "        res_max = max(res_max, temp_max)\n",
    "    return res_min * res_max"
   ]
  }
