    "import sys\n",
    "\n",
    "cnt = int(sys.stdin.readline())\n",
    "\n",
    "result = []\n",
    "for _ in range(cnt):\n",
    "    temp = int(sys.stdin.readline())\n",
    "    if temp  == 0 :\n",
    "        result.pop()\n",
    "    else :\n",
    "        result.append(temp)\n",
    "print(sum(result))"
   ]
