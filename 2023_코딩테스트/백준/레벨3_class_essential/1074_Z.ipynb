   "source": [
    "# 4배 단위로 재귀 !\n",
    "\n",
    "N, r, c = map(int, input().split())\n",
    "\n",
    "def sol(N, r, c):\n",
    "\n",
    "\tif N == 0:\n",
    "\t\treturn 0\n",
    "        \n",
    "\treturn 2*(r%2)+(c%2) + 4*sol(N-1, int(r/2), int(c/2))\n",
    "\n",
    "print(sol(N, r, c))"
   ]
  }
