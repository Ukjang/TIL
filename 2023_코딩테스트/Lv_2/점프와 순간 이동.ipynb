    "def solution(n):\n",
    "    # 2로 나눈 나머지들을 더해주면 소모량인듯..?\n",
    "    answer = 0\n",
    "    while True:\n",
    "        n_div = n // 2\n",
    "        n_rem = n % 2\n",
    "        answer += n_rem\n",
    "        if n_div == 0 :\n",
    "            break\n",
    "        n = n_div\n",
    "    return answer"
   ]
  }
