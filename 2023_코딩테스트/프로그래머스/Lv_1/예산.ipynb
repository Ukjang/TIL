    "def solution(d, budget):\n",
    "    # 오름차순 배열해서 갯구 구하면 될듯?\n",
    "    d.sort()\n",
    "    answer = 0\n",
    "    cnt = 0\n",
    "    for i in d :\n",
    "        if cnt + i <= budget :\n",
    "            answer += 1\n",
    "            cnt += i\n",
    "    return answer"
   ]
  }
