    "def solution(price, money, count):\n",
    "    cnt = sum([i for i in range(1, count + 1)])\n",
    "    total = money - (price * cnt)\n",
    "    if total >= 0 :\n",
    "        return 0\n",
    "    else :\n",
    "        return abs(total)"
